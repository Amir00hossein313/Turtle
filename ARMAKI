import math
def arc(a,r,angle):
    arc_length = 2*math.pi*r*angle/360
    n = int(arc_length/3)+1
    step_length = arc_length/n
    step_angle = angle/n
    for i in range(n):
        a.fd(step_length)
        a.lt(step_angle)

arc(a,28,218)

a.penup()
a.lt(55)
a.fd(40)
a.lt(90)
a.fd(10)
a.pendown()

arc(a,18,195)
a.lt(75)
a.fd(37)

a.penup()
a.rt(90)
